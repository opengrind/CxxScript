// Validation utilities
bool isPositive(int32 x) {
    return x > 0;
}

bool isInRange(int32 x, int32 min, int32 max) {
    return x >= min && x <= max;
}

bool validateAndCompute(int32 x) {
    if (isPositive(x)) {
        // Call external C++ function
        int32 result = validate(x);
        return result > 0;
    }
    return false;
}
